<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyComposeApp"
        tools:targetApi="31">
        <activity
            android:name=".MyComposeAppActivity"
            android:exported="false"
            android:label="@string/title_activity_my_compose_app"
            android:theme="@style/Theme.MyComposeApp">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".AffirmationActivity"
            android:exported="true"
            android:label="@string/title_activity_affirmation"
            android:theme="@style/Theme.MyComposeApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".TipTimeActivity"
            android:exported="false"
            android:label="@string/title_activity_tip_time"
