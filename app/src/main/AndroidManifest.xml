                android:value="" />
        </activity>
        <activity
            android:name=".DiceRollerActivity"
            android:exported="false"
            android:label="@string/title_activity_dice_roller"
            android:theme="@style/Theme.MyComposeApp">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".BirthdayCardActivity"
            android:exported="false"
            android:label="@string/title_activity_birthday_card"
            android:theme="@style/Theme.MyComposeApp">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.MyComposeApp">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
